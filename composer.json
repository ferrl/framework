{
  "name": "ferrl/framework",
  "description": "A set of tools built on top of laravel framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "homepage": "http://github.com/ferrl",
  "support": {
    "issues": "https://github.com/ferrl/framework/issues",
    "source": "https://github.com/ferrl/framework"
  },
  "authors": [
    {
      "name": "Lucas Ferreira",
      "email": "lucasferreirawd@gmail.com"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "laravel/framework": "~5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.3",
    "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "psr-4": {
      "Ferrl\\": "src/Ferrl/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "stub/application/app/"
    },
    "files": [
      "tests/TestCase.php"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
